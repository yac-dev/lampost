{
  "expo": {
    "name": "lampost",
    "slug": "lampost",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/lampost-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/lampost-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2A2B2C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lampost.lampost",
      "buildNumber": "1.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.lampost.lampost"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/splash.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d7972bf3-aa9a-4bfe-bfa3-2b0c344352d6"
      }
    }
  }
}
